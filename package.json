  "devDependencies": {
    "@types/cors": "^2.8.6",
    "apidoc": "^0.23.0",
    "csvtojson": "^2.0.10",
    "deep-freeze": "^0.0.1",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "minimist": "^0.2.1"
  },
